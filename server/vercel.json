{
    "installCommand" : "npm install",
    "version" : 2,
    "build":[
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}